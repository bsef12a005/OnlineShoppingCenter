@model pro.Models.admin
@{
    ViewBag.Title = "signUp1";
}

<h2>signUp1</h2>
@using (Html.BeginForm("signUp1","signUp1")){
    @Html.ValidationSummary(true)
    <fieldset>
        <legend> Email</Legend>
        <div class="editor-label">
            @Html.LabelFor(model=>model.email)</div>
         <div class="editor-field">
             @Html.EditorFor(model=>model.email)
              @Html.ValidationMessageFor(model=>model.email)</div>


        <legend> Password</Legend>
        <div class="editor-label">
            @Html.LabelFor(model=>model.password)</div>
         <div class="editor-field">
             @Html.EditorFor(model=>model.password)
              @Html.ValidationMessageFor(model=>model.password)</div>
        <legend> Phone No</Legend>
        <div class="editor-label">
            @Html.LabelFor(model=>model.phone)</div>
         <div class="editor-field">
             @Html.EditorFor(model=>model.phone)
              @Html.ValidationMessageFor(model=>model.phone)</div>


        <legend> City</Legend>
        <div class="editor-label">
            @Html.LabelFor(model=>model.city)</div>
         <div class="editor-field">
             @Html.EditorFor(model=>model.city)
              @Html.ValidationMessageFor(model=>model.city)</div>
        <p>
           <input type="submit"  value="Sign Up"/>
            <input type="button" value="Cancel" />
            </p>

        
        </fieldset>
}

